{
  "name": "typescript-nextjs-starter",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <hey@joaopedro.dev> (@jpedroschmitz)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "predev": "yarn run generate",
    "dev": "next",
    "dev:ts": "yarn lint && yarn dev & yarn ts:watch",
    "dev:https": "node server.js",
    "type-check": "tsc --noEmit --incremental",
    "ts:watch": "yarn type-check --watch",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "generate": "graphql-codegen --config codegen.yml",
    "seed": "ts-node ./modules/prisma/seed.ts",
    "delete": "ts-node ./modules/prisma/delete.ts"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.6.8",
    "@graphql-codegen/cli": "2.6.2",
    "@prisma/client": "4.8.0",
    "@reach/combobox": "0.18.0",
    "@sentry/nextjs": "7.86.0",
    "@stripe/react-stripe-js": "2.1.0",
    "@stripe/stripe-js": "1.38.1",
    "apollo-server-micro": "3.9.0",
    "axios": "1.4.0",
    "classnames": "2.3.2",
    "date-fns": "2.30.0",
    "date-fns-tz": "2.0.0",
    "deepmerge": "4.2.2",
    "email-validator": "2.0.4",
    "esbuild": "0.14.45",
    "focus-visible": "5.2.0",
    "framer-motion": "10.0.1",
    "graphql": "16.5.0",
    "graphql-tag": "2.12.6",
    "hamburger-react": "2.5.0",
    "jss-global": "3.0.0",
    "libphonenumber-js": "1.10.34",
    "lodash": "4.17.21",
    "luxon": "3.3.0",
    "micro": "9.3.4",
    "micro-cors": "0.1.1",
    "next": "14.0.4",
    "nexus": "1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.32.2",
    "react-jss": "10.9.0",
    "react-loading-skeleton": "3.3.1",
    "react-lottie": "1.2.3",
    "react-modal": "3.16.1",
    "react-phone-number-input": "3.2.23",
    "react-responsive-masonry": "2.1.7",
    "react-select": "5.7.3",
    "react-spring": "9.7.3",
    "recoil": "0.7.5",
    "rifm": "0.12.1",
    "stripe": "10.12.0",
    "tiny-skeleton-loader-react": "1.2.1",
    "use-places-autocomplete": "4.0.0",
    "uuid": "8.3.2",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-operations": "2.4.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.16",
    "@types/lodash": "4.14.182",
    "@types/luxon": "3.3.0",
    "@types/micro-cors": "0.1.2",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-lottie": "1.2.7",
    "@types/react-modal": "3.13.1",
    "@types/react-responsive-masonry": "2.1.3",
    "@types/uuid": "8.3.4",
    "@types/webfontloader": "1.6.35",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.28.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.31.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-watch": "8.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.0.2",
    "prettier": "2.7.1",
    "prisma": "4.8.0",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
